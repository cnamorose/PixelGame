  lines:
  - speaker: Devil
    text: "\uB4DC\uB514\uC5B4 \uB9CC\uB0AC\uAD70, \uC778\uAC04..."
    requiresName: 0
    shakeCamera: 0
  - speaker: Player
    text: "\uBB50\uC57C\u2026 \uB204\uAD6C\uC138\uC694? \uC0AC\uB78C\uC774 \uC544\uB2CC\uB370???"
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\uB108\uD76C \uC778\uAC04\uB4E4\uC740 \uB9E8\uB0A0 \uB204\uAD6C\uB0D0\uACE0
      \uBB3B\uB354\uB77C \u314B\u314B\u314B\u314B\u314B \\n \uB9DE\uC544 \uB09C \uC0AC\uB78C\uC774
      \uC544\uB2C8\uC57C"
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\uB098\uB294 \uC774 \uD559\uAD50\uC5D0 \uBD99\uC5B4 \uC0AC\uB294 \uC545\uB9C8,
      \uD398\uC77C\uC774\uB2E4."
    requiresName: 0
    shakeCamera: 0
  - speaker: Player
    text: "(\uC774\uB984 \uAC1C\uBCC4\uB85C\uB2E4... \uB204\uAC00 \uC9C0\uC740\uAC70\uC9C0)
      \uC545\uB9C8\uB77C\uACE0\uC694?"
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\uADF8\uB798, \uADF8\uB098\uC800\uB098 \uB108. \uC774\uB984\uC774 \uBB50\uC9C0?"
    requiresName: 0
    shakeCamera: 0
  - speaker: Player
    text: 
    requiresName: 1
    shakeCamera: 0
  - speaker: Devil
    text: "{name}...\uB0B4 \uB9C8\uC9C0\uB9C9 \uD76C\uC0DD\uC591\uC774 \uB418\uAE30
      \uB531 \uC88B\uC740 \uC774\uB984\uC774\uAD70 "
    requiresName: 0
    shakeCamera: 0
  - speaker: Player
    text: "??? \uD76C\uC0DD\uC591\uC774\uC694?"
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\uC88B\uC544, {name}. \uC124\uBA85\uD574\uC8FC\uACA0\uB2E4 "
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\uB098\uB294 \uC545\uB9C8\uC138\uACC4\uC5D0\uC11C '\uAD50\uC218' \uAC00
      \uB418\uAE30 \uC704\uD574 \\n\uB9C8\uC9C0\uB9C9 \uC870\uAC74\uC774 \uD544\uC694\uD558\uB2E4."
    requiresName: 0
    shakeCamera: 0
  - speaker: Player
    text: "\uAD50\uC218?"
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\uB2F9\uC5F0\uD558\uC9C0. \uAC70\uAE34 \uC778\uAC04 \uC138\uACC4\uBCF4\uB2E4
      \uBE61\uC138."
    requiresName: 0
    shakeCamera: 0
  - speaker: Player
    text: "\uADF8\uB7FC... \uC808 \uC8FD\uC5EC\uC11C \uB17C\uBB38\uC4F0\uB824\uACE0\uC694?!?"
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\uADF8\uAC74 \uC544\uB2C8\uACE0, \uAD50\uC218 \uC2B9\uC9C4 \uC870\uAC74\uC740
      \uAC04\uB2E8\uD558\uB2E4."
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\"10\uBA85\uC758 4\uD559\uB144 \uB208\uBB3C\"\uC744 \uBAA8\uC73C\uBA74
      \uB41C\uB2E4. "
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\uC878\uC5C5\uD559\uAE30\uC5D0 \uC878\uC5C5\uD558\uC9C0 \uBABB\uD55C \uC790\uB4E4\uC758
      \uB208\uBB3C\uC774\uC9C0!!!!!!!!!!"
    requiresName: 0
    shakeCamera: 1
  - speaker: Devil
    text: "\uB124 \uAC83\uB9CC \uBAA8\uC73C\uBA74 \uB531 10\uC138\uD2B8\uAC00 \uC644\uC131\uB41C\uB2E4. "
    requiresName: 0
    shakeCamera: 0
  - speaker: Player
    text: "...\uC7A0\uAE50\uB9CC\uC694, \uADF8\uB7FC \uC800 \uC878\uC5C5 \uBABB\uD558\uAC8C
      \uB9C9\uACE0\\n \uB208\uBB3C\uC744 \uAC00\uC838\uAC00\uACA0\uB2E4\uB294 \uAC70\uC608\uC694?"
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\uC774\uD574\uB97C \uC798 \uD558\uB294\uAD70, \uB098\uD55C\uD14C \uAC78\uB9B0
      \uD559\uC0DD\uB4E4\uC740 \\n \uD558\uB098\uAC19\uC774 \uD3EC\uAE30\uD558\uB354\uB77C!!!"
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\uBB3C\uB860, \uB108\uB3C4 \uADF8\uB807\uAC8C \uB420\uAC70\uB2E4."
    requiresName: 0
    shakeCamera: 0
  - speaker: Devil
    text: "\uC790, \uB17C\uBB38\uC744 \uC791\uC131\uD560 \uC218 \uC788\uB294 \uBB3C\uD488\uC740\\n
      \uC804\uBD80 \uD30C\uAD34\uD588\uB2E4."
    requiresName: 0
    shakeCamera: 0
  - speaker: Player
    text: "\uB0B4 \uCEF4\uD4E8\uD130\uB3C4???????!!!!!!!!"
    requiresName: 0
    shakeCamera: 1
  - speaker: Devil
    text: "\u314E\u314E\u314E... \uACFC\uC5F0 \uB10C \uC774 \uACE0\uBE44\uB97C \uB118\uAE30\uACE0
      \uC878\uC5C5 \uD560 \uC218 \uC788\uC744\uAE4C? "
    requiresName: 0
    shakeCamera: 0
  - speaker: Player
    text: "\uC544.. \uC548\uB3FC!!!! \uB098 \uAF2D \uC774\uBC88\uD559\uAE30\uC5D0
      \\n \uC878\uC5C5\uD574\uC57C\uD55C\uB2E4\uACE0!!!!!!"
    requiresName: 0
    shakeCamera: 1
  - speaker: Devil
    text: "\uC2DC\uC791\uD558\uC9C0. \\n {name}, \uB124 \uC878\uC5C5\uC758 \uC6B4\uBA85\uC740
      \uC774\uC81C \uB098\uC640 \uD568\uAED8\uD55C\uB2E4. "
    requiresName: 0
    shakeCamera: 0
